@*model List<CodingEventsDemo.Models.EventCategory>*@

<h1>@ViewBag.title</h1>


@if (@Model.Count == 0)
{
    <p>No categories yet!</p>
}
else
{
<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Name
        </th>

    </tr>

        @foreach (EventCategory eventCategories in Model)
         {
            <tr>
                <td>@eventCategories.Id</td>
                <td>@eventCategories.Name</td>

            </tr>
         }
</table>
}
