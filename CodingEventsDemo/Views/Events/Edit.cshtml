}
<h1>Edit Events</h1>

<form method="post" action="/events/edit">
    <input type="hidden" value="@ViewBag.events.Id" name="eventId" />
    
    <input name="name" type="text" value="@ViewBag.events.Name" />
    
    <input name="description" type="text" value="@ViewBag.events.Description"/>
    
    <input type="submit" value="Update" />
</form>

